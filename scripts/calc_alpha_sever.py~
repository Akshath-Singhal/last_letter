#!/usr/bin/env python

import rospy
from math import sqrt
from math import atan2
from math import pow
from math import pi
from uav_model.srv import *


def calc_alpha_callback(req):
	u = req.x
	v = req.y
	w = req.z
	
	alpha = atan2(w,u)

	return alpha

if __name__=='__main__':
	rospy.init_node('calc_alpha_server')
	s = rospy.Service('calc_alpha', calc_alpha, calc_alpha_callback)
	rospy.loginfo('calc_alpha service running')
	rospy.spin()
